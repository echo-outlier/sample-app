{
  "key": "my-app",
  "name": "My app",
  "description": "My very first app",
  "baseUrl": "{{localBaseUrl}}",
  "authentication": {
    "type": "jwt"
  },
  "lifecycle": {
    "installed": "/installed"
  },
  "scopes": ["READ"],
  "apiMigrations": {
    "signed-install": true
  },
  "modules": {
    "dynamicContentMacros": [
      {
        "key": "hello",
        "height": "1px",
        "width": "1px",
        "name": {
          "value": "Hello World App"
        },
        "url": "/audio",
        "description": {
          "value": "Audio Recorder App"
        },
        "outputType": "inline",
        "bodyType": "none"
      }
    ],
    "configurePage": {
      "url": "/my-config-page",
      "name": {
        "value": "My Configure Page"
      },
      "key": "my-config-page"
    }
  }
}
